import "accounts"

abstract model BaseModel {
  id            String       @id @unique @default(uuid())    // Use a UUID data type for the ID
  createdAt     DateTime     @default(now())                 // Automatically set the creation timestamp
  updatedAt     DateTime     @updatedAt                      // Automatically set the update timestamp
}

model File extends BaseModel {
  resourceType  String

  // Not needed but compulsory according to design
  users  User[]
}